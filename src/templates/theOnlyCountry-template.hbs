{{#each this}}

    <h2 class = 'country-title'>{{name}}</h2>

    <div class='country-container'>
        <div class='country-info'>
    <p> <span class= 'span-item'>Capital</span>: {{capital}}</p>
    <p><span class= 'span-item'>Population</span> {{population}}</p>
    <ul class='languages-list'><span class= 'span-item'>Languages</span>: {{#each languages}}
        <li>{{name}}</li>
     {{/each}}
    </ul>
    </div>

    <img src="{{flag}}" width="350">
    </div>
{{/each}}